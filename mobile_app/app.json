{
  "server_base_url": "https://github.com/RyanJulyan/Flask-BDA",
  "expo": {
    "name": "Flask BDA",
    "slug": "flask-bda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#ffffff",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
        [
            "expo-notifications",
            {
              "icon": "./assets/adaptive-icon.png",
              "color": "#ffffff",
              "sounds": ["./assets/notification_alert.mp3"],
              "mode": "production"
            }
        ]
    ]
  }
}
